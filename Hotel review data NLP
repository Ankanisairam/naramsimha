   "source": [
    "# Apply a second round of clearning\n",
    "def text_clean_2(text):\n",
    "    text = re.sub('[''\"\",,,,]','',text)\n",
    "    text = re.sub('\\n','',text)\n",
    "    return text\n",
    "\n",
    "cleaned2 = lambda x:text_clean_2 (x)"
   ]
  },
